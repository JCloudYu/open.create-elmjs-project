{
	"compilerOptions": {
		"outFile": "./_build/boot.js",
		"rootDir": ".",
		"target": "es2022",
		"module": "amd",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"paths": {
			"/*": [ "./*" ]
		}
	},
	"exclude": [ "./_temp", "./_build" ]
}